{
  "name": "dataflex-syntax-highlighter",
  "displayName": "DataFlex Syntax Highlighter",
  "description": "Syntax highlighting support for the DataFlex programming language.",
  "version": "0.2.0",
  "publisher": "nilssve",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "dataflex",
        "aliases": ["DataFlex"],
        "extensions": [".src", ".pkg", ".vw", ".dg", ".sl", ".bp", ".wo", ".inc", ".rv", ".dd"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "dataflex",
        "scopeName": "source.dataflex",
        "path": "./syntaxes/dataflex.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NilsSve/dataflex-syntax-highlighter"
  }
}
